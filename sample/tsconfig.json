{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@c/*": ["src/common/*"],
      "@g/*": ["src/gas/*"],
      "@v/*": ["src/vue/*"],
    },
    "lib": [
      "dom",
      "esnext"
    ],
    "types": [
      "google-apps-script"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.vue"
  ]
}